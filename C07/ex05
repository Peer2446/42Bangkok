// Online C compiler to run C program online
#include <stdio.h>
#include <stdlib.h>
int countcharset(char *str, char *charset)
{
    int count;
    int j;
    
    count = 0;
    while (*str)
    {
        j = 0;
		while (str[j] == charset[j] && charset[j] && str[j])
		{
			j++;
		}
		if (charset[j] == '\0')
			count++;
		str++;
    }
    return (count);
}
char **ft_split(char *str, char *charset)
{
    int c;
    char **tmp;
    
    tmp = malloc(countcharset(str, charset));
    return (tmp);
}

int main()
{
    char *str = "aafe, fefsdf, gfeg, hhhh";
    char **a= ft_split(str, ", ");
    //printf("%d",countcharset(str,", "));
}
